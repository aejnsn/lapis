{
    "name": "aejnsn/lapis",
    "description": "An API toolkit for Laravel 5.5.",

    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "api",
        "toolkit",
        "filtering"
    ],
    "authors": [
        {
            "name": "Aaron Johnson",
            "email": "aaron@aej.io"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/database": "5.5.*",
        "illuminate/support": "5.5.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "graham-campbell/testbench": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Aejnsn\\Lapis\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Aejnsn\\Lapis\\Tests\\": "tests/"
        }
    }
}
